{
  "branches": ["main"],
  "plugins": [
    [
      "@semantic-release/release-notes-generator",
      {
        "preset": "conventionalcommits",
        "presetConfig": {
          "types": [
            {"type": "feat", "section": "‚ú® Features"},
            {"type": "fix", "section": "üêõ Fixes"},
            {"type": "docs", "section": "üìù Docs"},
            {"type": "refactor", "section": "‚ôªÔ∏è Refactoring"},
            {"type": "perf", "section": "‚ö° Performance"},
            {"type": "test", "section": "‚úÖ Tests"},
            {"type": "chore", "section": "üîß Chores"}
          ]
        }
      }
    ],
    [
      "@semantic-release/changelog",
      {
        "changelogFile": "CHANGELOG.md",
        "changelogTitle": "# Changelog\n\nGenerated with GitVersion ‚ù§Ô∏è semantic-release"
      }
    ],
    "@semantic-release/github",
    [
      "@semantic-release/git",
      {
        "assets": ["CHANGELOG.md"],
        "message": "chore(changelog): update changelog for v${nextRelease.version} [skip ci]"
      }
    ]
  ]
}
